<Window x:Class="JobStarter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JobStarter"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="CommandGrid" Grid.Row="1" Margin="0,0,350,300">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Id" IsReadOnly="True"/>
                <DataGridTextColumn Header="Komenda"/>
            </DataGrid.Columns>
        </DataGrid>

        <Button x:Name="AddRow" Content="Dodaj" HorizontalAlignment="Left" Margin="85,139,0,0" VerticalAlignment="Top"/>
        <Button x:Name="RemoveRow" Content="Usun" HorizontalAlignment="Left" Margin="126,139,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
